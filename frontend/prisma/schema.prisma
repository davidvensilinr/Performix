generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model organisation {
  id         Int    @id @default(autoincrement())
  name       String @db.VarChar(500)
  managed_by String @db.VarChar(100)
}
